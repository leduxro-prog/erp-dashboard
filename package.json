{
  "name": "cypher-erp",
  "version": "0.1.0",
  "description": "CYPHER ERP/CRM System for Ledux.ro â€” LED Lighting E-Commerce",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && tsc-alias",
    "build:incremental": "tsc --incremental --tsBuildInfoFile .tsbuildinfo && tsc-alias",
    "start": "node dist/server.js",
    "lint": "eslint 'src/**/*.ts' 'shared/**/*.ts' 'modules/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts' 'shared/**/*.ts' 'modules/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'shared/**/*.ts' 'modules/**/*.ts'",
    "test": "NODE_OPTIONS=--max-old-space-size=2048 jest --passWithNoTests",
    "test:watch": "NODE_OPTIONS=--max-old-space-size=2048 jest --watch",
    "test:coverage": "NODE_OPTIONS=--max-old-space-size=2048 jest --coverage",
    "test:changed": "node scripts/test-changed-modules.js",
    "test:integration": "NODE_OPTIONS=--max-old-space-size=2048 jest --config jest.integration.config.ts",
    "test:smoke": "jest --testPathPattern=tests/smoke --verbose",
    "test:rollback-drill": "jest --testPathPattern=tests/rollback --verbose",
    "test:critical": "npm run test:integration",
    "test:e2e": "NODE_OPTIONS=--max-old-space-size=2048 jest --config jest.e2e.config.ts",
    "test:e2e:eventbus": "NODE_OPTIONS=--max-old-space-size=2048 jest --config jest.e2e.config.ts tests/e2e/eventbus/EventBusE2ETest.ts",
    "test:e2e:jobs": "NODE_OPTIONS=--max-old-space-size=2048 jest --config jest.e2e.config.ts tests/e2e/jobs/JobsE2ETest.ts",
    "ci:green": "npm run build && npm --prefix frontend run build && npm run test:critical",
    "ci:fast": "npm run lint && npm --prefix frontend run lint && npm run build:incremental && npm --prefix frontend run build && npm run test:changed",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/data-source.ts",
    "seed": "ts-node database/seeds/index.ts",
    "deploy:safe": "bash scripts/deploy-safe-app.sh",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.1.13",
    "@nestjs/core": "^11.1.13",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/typeorm": "^11.0.0",
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "amqplib": "^0.10.3",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.4.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^3.0.0-rc.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^8.0.1",
    "otplib": "^12.0.1",
    "pdfkit": "^0.15.0",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.0.0",
    "rxjs": "^7.8.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/amqplib": "^0.10.4",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.25",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.9",
    "@types/pdfkit": "^0.17.4",
    "@types/pg": "^8.16.0",
    "@types/puppeteer": "^5.4.7",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "ajv": "^8.18.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3"
  }
}
